{
  "expo": {
    "name": "ai-companion-app",
    "slug": "ai-companion-app",
    "scheme": "ai-companion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AI_Companion_icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/AI_Companion_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ronnie39.aicompanionapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AI_Companion_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ronnie39.aicompanionapp"
    },
    "web": {
      "favicon": "./assets/AI_Companion_icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-audio",
      "expo-video"
    ]
  }
}
