{
  "expo": {
    "name": "Asteron",
    "slug": "asteron",
    "scheme": "asteron",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AI_Companion_icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/AI_Companion_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.asteron",
      "usesAppleSignIn": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.754044842286-qdjnjk0lasj1bnrm30i4b7mltk6bgp4a"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AI_Companion_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "app.asteron"
    },
    "web": {
      "favicon": "./assets/AI_Companion_icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-audio",
      "expo-video",
      "@react-native-community/datetimepicker",
      "expo-web-browser",
      "expo-notifications",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication"
    ]
  }
}
